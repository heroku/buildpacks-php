{
    "config": {
        "allow-plugins": {
            "heroku/installer-plugin": true
        },
        "cache-files-ttl": 0,
        "discard-changes": true
    },
    "minimum-stability": "stable",
    "prefer-stable": false,
    "provide": {
        "heroku-sys\/heroku": "20."
    },
    "require": {
        "heroku/installer-plugin": "^1.8.6",
        "heroku-sys\/composer": "*",
        "heroku-sys\/composer-plugin-api": "^1",
        "moontoast\/math": "1.2.1",
        "paragonie\/constant_time_encoding": "v2.4.0",
        "paragonie\/random_compat": "v9.99.100",
        "phpseclib\/bcmath_compat": "2.0.0",
        "phpseclib\/phpseclib": "3.0.10"
    },
    "repositories": [
        {
            "packagist.org": false
        },
        {
            "type": "composer",
            "url": "https:\/\/lang-php.s3.us-east-1.amazonaws.com\/dist-heroku-20-cnb\/packages.json"
        },
        {
            "type": "package",
            "package": [
                {
                    "type": "metapackage",
                    "name": "moontoast\/math",
                    "version": "1.2.1",
                    "require": {
                        "heroku-sys\/php": ">=5.3.3"
                    }
                },
                {
                    "type": "metapackage",
                    "name": "paragonie\/constant_time_encoding",
                    "version": "v2.4.0",
                    "require": {
                        "heroku-sys\/php": "^7|^8"
                    }
                },
                {
                    "type": "metapackage",
                    "name": "paragonie\/random_compat",
                    "version": "v9.99.100",
                    "require": {
                        "heroku-sys\/php": ">= 7"
                    }
                },
                {
                    "type": "metapackage",
                    "name": "phpseclib\/bcmath_compat",
                    "version": "2.0.0",
                    "provide": {
                        "heroku-sys\/ext-bcmath": "8.0.0"
                    }
                },
                {
                    "type": "metapackage",
                    "name": "phpseclib\/phpseclib",
                    "version": "3.0.10",
                    "require": {
                        "heroku-sys\/php": ">=5.6.1"
                    }
                }
            ]
        }
    ]
}
