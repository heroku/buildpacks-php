{
    "config": {
        "allow-plugins": {
            "heroku/installer-plugin": true
        },
        "cache-files-ttl": 0,
        "discard-changes": true
    },
    "minimum-stability": "stable",
    "prefer-stable": false,
    "provide": {
        "heroku-sys\/heroku": "20."
    },
    "require": {
        "heroku/installer-plugin": "*",
        "heroku-sys\/apache": "^2.4.10",
        "heroku-sys\/nginx": "^1.8.0",
        "heroku-sys\/composer": "*",
        "heroku-sys\/composer-plugin-api": "^2",
        "heroku/heroku-buildpack-php": "dev-master",
        "symfony\/polyfill-ctype": "v1.24.0"
    },
    "repositories": [
        {
            "packagist.org": false
        },
        {
            "type": "path",
            "url": "..\/..\/support\/installer",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../support/installer/../..",
            "options": {
                "symlink": false,
                "versions": {
                    "heroku/heroku-buildpack-php": "dev-master"
                }
            }
        },
        {
            "type": "composer",
            "url": "https:\/\/lang-php.s3.us-east-1.amazonaws.com\/dist-heroku-20-cnb\/packages.json"
        },
        {
            "type": "package",
            "package":
            {
                "type": "metapackage",
                "name": "symfony\/polyfill-ctype",
                "version": "v1.24.0",
                "require": {
                    "heroku-sys\/php": ">=7.1"
                },
                "provide": {
                    "heroku-sys/ext-ctype": "*"
                }
            }
        }
    ]
}
